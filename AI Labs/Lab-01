#List Slicing
name_list=["Zain","Sharjil","Hamza","Aurang","Ammar"]
print (name_list[1:2])
['Sharjil']
print (name_list[0:3])
['Zain', 'Sharjil', 'Hamza']
print (name_list[:4])
['Zain', 'Sharjil', 'Hamza', 'Aurang']
print (name_list[1:-2])
['Sharjil', 'Hamza']
print (name_list[:])
['Zain', 'Sharjil', 'Hamza', 'Aurang', 'Ammar']

